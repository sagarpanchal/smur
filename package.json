{
  "name": "smurr",
  "version": "1.0.8",
  "description": "A ExpressJS REST API server with Socket.io, Mongodb, Redis, and Yup built-in for headstart. Check README for more info.",
  "main": "index.js",
  "scripts": {
    "start": "npm run development",
    "dev": "npm run development",
    "predevelopment": "npm run lint && npm run test",
    "development": "node_modules/.bin/cross-env NODE_PATH=. NODE_ENV=development DEBUG=app:* INIT=0 node development.js --use-strict",
    "prod": "npm run production",
    "prod-watch": "npm run production -- --watch",
    "prod-monit": "node_modules/.bin/pm2 monit",
    "prod-stop": "node_modules/.bin/pm2 stop driven-to-impact-server",
    "preproduction": "npm run lint && npm run test",
    "production": "node_modules/.bin/cross-env NODE_PATH=. NODE_ENV=production NODE_CONFIG_STRICT_MODE=0 DEBUG= node_modules/.bin/pm2 start ./index.js -n 'driven-to-impact-server' -i max",
    "lint": "node_modules/.bin/eslint . --ignore-path .gitignore",
    "test": "node_modules/.bin/cross-env NODE_ENV=development NODE_PATH=. DEBUG=test:* node_modules/.bin/jest --verbose --passWithNoTests"
  },
  "keywords": [
    "express",
    "socket.io",
    "mongoose",
    "ioredis"
  ],
  "author": "Sagar Panchal <panchal.sagar@outlook.com>",
  "license": "AGPL",
  "dependencies": {
    "aws-sdk": "^2.673.0",
    "bcryptjs": "^2.4.3",
    "bson": "^4.0.4",
    "chalk": "^4.0.0",
    "compression": "^1.7.4",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^3.22.0",
    "ioredis": "^4.16.3",
    "ioredis-lock": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mime": "^2.4.5",
    "moment": "^2.25.3",
    "mongoose": "^5.9.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.6",
    "open": "^7.0.3",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-mixer": "^1.0.2",
    "passport-oauth2": "^1.5.0",
    "passport-twitch-new": "0.0.1",
    "pm2": "^4.4.0",
    "pug": "^2.0.4",
    "sharp": "^0.25.2",
    "socket.io": "^2.3.0",
    "socket.io-redis": "^5.2.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.0.0",
    "winston": "^3.2.1",
    "winston-mongodb": "^5.0.1",
    "winston-transport": "^4.3.0",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-esnext": "^4.0.0",
    "eslint-config-node": "^4.0.0",
    "jest": "^26.0.1",
    "nodemon": "2.0.3"
  }
}
