{
  "name": "smur",
  "version": "1.0.8",
  "description": "A ExpressJS REST API server with Socket.io, Mongodb, Redis, and Yup built-in for headstart. Check README for more info.",
  "engines": {
    "node": ">=14.5.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm run development",
    "dev": "npm run development",
    "predevelopment": "npm run test",
    "development": "node_modules/.bin/cross-env NODE_PATH=. NODE_ENV=development DEBUG=app:* INIT=0 node development.js --use-strict",
    "prod": "npm run production",
    "prod-watch": "npm run production -- --watch",
    "prod-monit": "node_modules/.bin/pm2 monit",
    "prod-stop": "node_modules/.bin/pm2 stop smur",
    "preproduction": "npm run lint && npm run test",
    "production": "node_modules/.bin/cross-env NODE_PATH=. NODE_ENV=production NODE_CONFIG_STRICT_MODE=0 DEBUG= node_modules/.bin/pm2 start ./index.js -n 'smur' -i max",
    "lint": "node_modules/.bin/eslint .",
    "prettier-write": "node_modules/.bin/prettier --write \"./**/*.{js,json,yaml,md}\"",
    "eslint-fix": "node_modules/.bin/eslint --fix",
    "format": "npm run prettier-write && npm run eslint-fix",
    "test": "node_modules/.bin/cross-env NODE_ENV=development NODE_PATH=. DEBUG=test:* node_modules/.bin/jest --verbose --passWithNoTests"
  },
  "keywords": [
    "express",
    "socket.io",
    "mongoose",
    "ioredis"
  ],
  "author": "Sagar Panchal <panchal.sagar@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.912.0",
    "bcryptjs": "^2.4.3",
    "bson": "^4.4.0",
    "chalk": "^4.1.1",
    "compression": "^1.7.4",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "^4.3.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "fs-extra": "^10.0.0",
    "helmet": "^4.6.0",
    "html-minifier": "^4.0.0",
    "ioredis": "^4.27.3",
    "ioredis-lock": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "mime": "^2.5.2",
    "moment": "^2.29.1",
    "mongoose": "^5.12.10",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.6.0",
    "open": "^8.1.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-oauth2": "^1.5.0",
    "pm2": "^4.5.6",
    "pug": "^3.0.2",
    "sharp": "^0.28.2",
    "socket.io": "^4.1.2",
    "socket.io-redis": "^6.1.1",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-mongodb": "^5.0.7",
    "winston-transport": "^4.4.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "nodemon": "2.0.7",
    "prettier": "^2.3.0"
  }
}
