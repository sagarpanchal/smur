{
  "name": "smur",
  "version": "1.0.8",
  "description": "A ExpressJS REST API server with Socket.io, Mongodb, Redis, and Yup built-in for headstart. Check README for more info.",
  "engines": {
    "node": ">=14.5.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm run development",
    "dev": "npm run development",
    "predevelopment": "npm run test",
    "development": "node_modules/.bin/cross-env NODE_PATH=. NODE_ENV=development DEBUG=app:* INIT=0 node development.js --use-strict",
    "prod": "npm run production",
    "prod-watch": "npm run production -- --watch",
    "prod-monit": "node_modules/.bin/pm2 monit",
    "prod-stop": "node_modules/.bin/pm2 stop driven-to-impact-server",
    "preproduction": "npm run lint && npm run test",
    "production": "node_modules/.bin/cross-env NODE_PATH=. NODE_ENV=production NODE_CONFIG_STRICT_MODE=0 DEBUG= node_modules/.bin/pm2 start ./index.js -n 'driven-to-impact-server' -i max",
    "lint": "node_modules/.bin/eslint .",
    "prettier-write": "node_modules/.bin/prettier --write \"./**/*.{js,json,yaml,md}\"",
    "eslint-fix": "node_modules/.bin/eslint --fix",
    "format": "npm run prettier-write && npm run eslint-fix",
    "test": "node_modules/.bin/cross-env NODE_ENV=development NODE_PATH=. DEBUG=test:* node_modules/.bin/jest --verbose --passWithNoTests"
  },
  "keywords": [
    "express",
    "socket.io",
    "mongoose",
    "ioredis"
  ],
  "author": "Sagar Panchal <panchal.sagar@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.771.0",
    "bcryptjs": "^2.4.3",
    "bson": "^4.1.0",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "config": "^3.3.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.1.1",
    "ioredis": "^4.17.3",
    "ioredis-lock": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "mime": "^2.4.6",
    "moment": "^2.29.1",
    "mongoose": "^5.10.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.13",
    "open": "^7.3.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-oauth2": "^1.5.0",
    "pm2": "^4.5.0",
    "pug": "^3.0.0",
    "sharp": "^0.26.1",
    "socket.io": "^2.3.0",
    "socket.io-redis": "^5.4.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "winston-mongodb": "^5.0.5",
    "winston-transport": "^4.4.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.3",
    "nodemon": "2.0.4",
    "prettier": "^2.1.2"
  }
}
